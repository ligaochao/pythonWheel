        // 指定图表的配置项和数据
        var option = {
     tooltip: {
                formatter: function(params, ticket, callback) {
                    return dataArr[params.dataIndex] + "<br />" + 
                    params.seriesName + "：" + params.value;
                }
            },
            grid: {
