                    lineStyle:{
                        color:'green'
                    }
                },
                data: ['2015-09-09', '2015-09-11', '2015-09-14', '2015-09-17', '2015-09-21', '2015-09-22', '2015-01-01', '2015-09-24', '2015-10-09', '2015-09-16', '2015-09-18', '2015-10-10', '2015-10-11', '2015-10-12', '2015-10-13', '2015-10-14', '2015-10-15', '2015-10-16', '2015-10-17', '2015-10-18', '2015-10-19', '2015-10-20', '2015-10-21', '2015-10-22', '2015-10-23', '2015-10-24', '2015-10-25', '2015-10-26', '2015-10-27', '2015-10-28', '2015-10-29', '2015-10-30', '2015-10-31', '2015-11-01', '2015-11-02', '2015-11-03', '2015-11-04', '2015-11-05', '2015-11-06', '2015-11-07', '2015-11-08', '2015-11-09', '2015-11-10', '2015-11-11', '2015-11-12', '2015-11-13', '2015-11-14', '2015-11-15', '2015-11-16', '2015-11-17', '2015-11-18', '2015-11-19', '2015-11-20', '2015-11-21', '2015-11-22', '2015-11-23', '2015-11-24', '2015-11-25', '2015-11-26', '2015-11-27', '2015-11-28', '2015-11-29', '2015-11-30', '2015-12-01', '2015-12-02']
            },
            yAxis: [{
                type: 'value',
                axisLabel:{
                    show: true,
                    textStyle: {
                        color: 'blue'
                    }
                },
                axisLine:{
                    show:true,
                    lineStyle:{
                        color:'blue'
                    }
                }
            }],
            series:[{
                    name:brokenName[0],
                    type:'line',
                    lineStyle:{
                        normal:{
                            width:2,
                            color:'blue'
                        }
                    },
                    data:brokenData[0]
