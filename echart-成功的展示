                    data:brokenData[2]

            },{
                    name:brokenName[3],
                    type:'line',
                    lineStyle:{
                        normal:{
                            width:2,
                            color:'yellow'
                        }
                    },
                    data:brokenData[3]
            }]
        };

        // 使用刚指定的配置项和数据显示图表。
        myChart.setOption(option);
    </script>
