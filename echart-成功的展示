                    name:brokenName[1],
                    type:'line',
                    lineStyle:{
                        normal:{
                            width:2,
                            color:'black'
                        }
                    },
                    data:brokenData[1]

            },{
                    name:brokenName[2],
                    type:'line',
                    lineStyle:{
                        normal:{
                            width:2,
                            color:'red'
                        }
